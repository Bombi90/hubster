{
  "name": "hubster-js",
  "version": "0.0.8",
  "author": {
    "name": "Bombi90"
  },
  "description": "Javascript loader that manages applications lifecycle",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "unpkg": "lib/hubster.min.js",
  "repository": "Bombi90/hubster",
  "license": "MIT",
  "scripts": {
    "build": "parcel build ./src/index.ts --global --detailed-report --target browser --bundle-node-modules --no-source-maps  --out-file hubster.min.js --out-dir ./dist/",
    "watch": "tsc --watch",
    "types:check": "tsc --noEmit",
    "lint": "eslint '{src,test}/**/*.ts'",
    "lint:fix": "eslint '{src,test}/**/*.ts' --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run types:check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "npm run lint:fix"
    ]
  },
  "engines": {
    "node": ">=7.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.9.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "@zeit/ncc": "^0.21.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "jest": "^25.0.0",
    "lint-staged": "^10.0.8",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "@babel/runtime-corejs2": "^7.8.7",
    "inversify": "^5.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "reflect-metadata": "^0.1.13"
  }
}
